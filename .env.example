# Google Sheets Configuration
SHEET_ID=your_google_sheet_id_here

# Google Business Profile Configuration
GOOGLE_BUSINESS_ACCOUNT_ID=accounts/your_account_id_here

# 7shifts API Configuration
SEVENSHIFT_API_KEY=your_7shifts_api_key_here
SEVENSHIFT_COMPANY_ID=your_company_id_here

# Restaurant Location IDs (from Google My Business)
# Format: locations/{location_id}
# The account ID will be automatically prepended
MARGIES_LOCATION_ID=locations/your_margies_location_id
GRACKLE_LOCATION_ID=locations/your_grackle_location_id

# Optional: Logging level
LOG_LEVEL=INFO
